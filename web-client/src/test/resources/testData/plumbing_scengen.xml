  <type typeid="9" name="List of Timestamp"/>

  <unit unitid="1" typeid="1" name="Unit/Double"/>
  <unit unitid="2" typeid="2" name="Unit/Integer"/>
  <unit unitid="3" typeid="3" name="Unit/String"/>
  <unit unitid="4" typeid="4" name="Unit/Timestamp"/>
  <unit unitid="5" typeid="5" name="Unit/TimeSeries/step"/>
  <unit unitid="6" typeid="6" name="Unit/TimeSeries/linear"/>
  <unit unitid="7" typeid="7" name="Unit/List of Double"/>
  <unit unitid="8" typeid="8" name="Unit/List of Integer"/>
  <unit unitid="9" typeid="9" name="Unit/List of Timestamp"/>

  <component componentid="0" prjid="1" name="dummy"/>
  <component componentid="1" prjid="2" name="C01"/>
  <component componentid="2" prjid="2" name="PIP01"/>
  <component componentid="3" prjid="2" name="CITYOPT"/>

  <inputparameter inputid="1" name="typ" unitid="2" componentid="1" defaultvalue="2"/>
  <inputparameter inputid="2" componentid="3" name="simulation_start" unitid="4" defaultvalue="2015-01-01T00:00:00"/>
  <inputparameter inputid="3" componentid="3" name="simulation_end" unitid="4" defaultvalue="2015-01-01T00:00:00"/>

  <outputvariable outvarid="1" name="PI12_MIX_MASS_FLOW" unitid="6" componentid="2"/>

  <extparam extparamid="1" prjid="2" defaultvalue="100" name="d" unitid="1"/>

  <extparamvalset extparamvalsetid="1"/>
  <extparamval extparamvalid="1" extparamid="1" value="100.0"/>
  <extparamvalsetcomp id="1" extparamvalsetid="1" extparamvalid="1"/>

  <metric prjid="1" name="dummy" unitid="1" expression="C01.typ"/>
  <metric prjid="2" name="symmetric" unitid="1" expression="d + C01.typ + PIP01.PI12_MIX_MASS_FLOW.values[-1]"/>

  <objectivefunction obtfunctionid="1" prjid="2" typeid="1" name="obj1" expression="PIP01.PI12_MIX_MASS_FLOW * C01.typ" ismaximise="true"/>

  <optconstraint optconstid="1" prjid="2" name="con1" expression="mean(PIP01.PI12_MIX_MASS_FLOW)" lowerbound="0.1" upperbound="1.0"/>

  <scenariogenerator scengenid="1" prjid="2" algorithmid="1" extparamvalsetid="1"/>
  <scengenobjectivefunction sgobfunctionid="1" scengenid="1" optfunctionid="1"/>
  <scengenoptconstraint sgoptconstraintid="1" scengenid="1" optconstid="1"/>

  <decisionvariable decisionvarid="1" scengenid="1" inputid="1" lowerbound="1" upperbound="2" typeid="2"/>

  <modelparameter modelparamid="2" scengenid="1" inputid="2" value="2015-01-01T00:00:00" expression=""/>
  <modelparameter modelparamid="3" scengenid="1" inputid="3" value="2015-01-01T00:01:00"/>
  <modelparameter modelparamid="1" scengenid="1" inputid="1" expression="C01.typ"/>

  <algoparamval aparamsid="1" ascengenid="1" value="1"/>
  <algoparamval aparamsid="2" ascengenid="1" value="1000"/>


</dataset>
