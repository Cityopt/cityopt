<?xml version="1.0" encoding="UTF-8"?>

<dataset>

  <simulationmodel modelid="1" description="Plumbing test model" modelblob="" simulator="Apros-Combustion-5.13.06-64bit" timeorigin="2015-01-01 00:00:00"/>

  <project prjid="1" name="Project 1" location="dummy" modelid="1"/>
  <project prjid="2" name="Plumbing test project" location="Helsinki" modelid="1"/>

  <algorithm algorithmid="1" description="grid search"/>
  <algoparam aparamsid="1" algorithmid="1" name="max runtime [minutes]"/>
  <algoparam aparamsid="2" algorithmid="1" name="max variable domain size"/>

  <type typeid="1" name="Double"/>
  <type typeid="2" name="Integer"/>
  <type typeid="3" name="String"/>
  <type typeid="4" name="Timestamp"/>
  <type typeid="5" name="TimeSeries/step"/>
  <type typeid="6" name="TimeSeries/linear"/>
  <type typeid="7" name="List of Double"/>
