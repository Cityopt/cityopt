<?xml version="1.0" encoding="UTF-8"?>

<dataset>

  <simulationmodel modelid="1" description="Plumbing test model" modelblob="" simulator="Apros-Combustion-5.13.06-64bit" timeorigin="2015-01-01 00:00:00" version="1"/>

  <extparamvalset extparamvalsetid="0" name="defaults" version="1"/>
  <extparamvalset extparamvalsetid="1" name="scengentest" version="1"/>

  <project prjid="1" name="Project 1" location="dummy" modelid="1" version="1"/>
  <project prjid="2" name="Plumbing test project" location="Helsinki" modelid="1" defaultextparamvalsetid="0" version="1"/>

  <algorithm algorithmid="1" description="grid search" version="1"/>
  <algoparam aparamsid="1" algorithmid="1" name="max runtime [minutes]" defaultValue="1000000" version="1"/>
  <algoparam aparamsid="2" algorithmid="1" name="max scenarios" defaultValue="100" version="1"/>

  <algorithm algorithmid="2" description="genetic algorithm" version="1"/>
  <algoparam aparamsid="3" algorithmid="2" name="max runtime [minutes]" defaultValue="1000000" version="1"/>
  <algoparam aparamsid="4" algorithmid="2" name="number of generations" defaultValue="100" version="1"/>
  <algoparam aparamsid="5" algorithmid="2" name="population size" defaultValue="100" version="1"/>
  <algoparam aparamsid="6" algorithmid="2" name="number of parents per generation" defaultValue="25" version="1"/>
  <algoparam aparamsid="7" algorithmid="2" name="number of offspring per generation" defaultValue="25" version="1"/>
  <algoparam aparamsid="8" algorithmid="2" name="seed of the random number generator" defaultValue="0.95" version="1"/>

  <type typeid="1" name="Double" version="1"/>
  <type typeid="2" name="Integer" version="1"/>
  <type typeid="3" name="String" version="1"/>
  <type typeid="4" name="Timestamp" version="1"/>
  <type typeid="5" name="TimeSeries/step" version="1"/>
  <type typeid="6" name="TimeSeries/linear" version="1"/>
  <type typeid="7" name="List of Double" version="1"/>
  <type typeid="8" name="List of Integer" version="1"/>
  <type typeid="9" name="List of Timestamp" version="1"/>

  <unit unitid="1" name="Unit/Double" version="1"/>
  <unit unitid="2" name="Unit/Integer" version="1"/>
  <unit unitid="3" name="Unit/String" version="1"/>
  <unit unitid="4" name="Unit/Timestamp" version="1"/>
  <unit unitid="5" name="Unit/TimeSeries/step" version="1"/>
  <unit unitid="6" name="Unit/TimeSeries/linear" version="1"/>
  <unit unitid="7" name="Unit/List of Double" version="1"/>
  <unit unitid="8" name="Unit/List of Integer" version="1"/>
  <unit unitid="9" name="Unit/List of Timestamp" version="1"/>

  <component componentid="0" prjid="1" name="dummy" version="1"/>
  <component componentid="1" prjid="2" name="C01" version="1"/>
  <component componentid="2" prjid="2" name="PIP01" version="1"/>
  <component componentid="3" prjid="2" name="CITYOPT" version="1"/>

  <inputparameter inputid="1" name="typ" unitid="2" typeid="2" componentid="1" defaultvalue="2" version="1"/>
  <inputparameter inputid="2" componentid="3" name="simulation_start" unitid="4" typeid="4" defaultvalue="2015-01-01T00:00:00" version="1"/>
  <inputparameter inputid="3" componentid="3" name="simulation_end" unitid="4" typeid="4" defaultvalue="2015-01-01T00:00:00" version="1"/>

  <outputvariable outvarid="1" name="PI12_MIX_MASS_FLOW" unitid="6" typeid="6" componentid="2" version="1"/>

  <extparam extparamid="1" prjid="2" name="d" unitid="1" typeid="1" version="1"/>

  <extparamval extparamvalid="0" extparamid="1" value="99" version="1"/>
  <extparamval extparamvalid="1" extparamid="1" value="100.0" version="1"/>

  <extparamvalsetcomp extparamvalsetid="0" extparamvalid="0" version="1"/>
  <extparamvalsetcomp extparamvalsetid="1" extparamvalid="1" version="1"/>

  <metric prjid="1" name="dummy" unitid="1" typeid="1" expression="C01.typ" version="1"/>
  <metric prjid="2" name="symmetric" unitid="1" typeid="1" expression="d + C01.typ + PIP01.PI12_MIX_MASS_FLOW.values[-1]" version="1"/>

  <objectivefunction obtfunctionid="1" prjid="2" typeid="1" name="obj1" expression="integrate(PIP01.PI12_MIX_MASS_FLOW * C01.typ, 0, Infinity)" ismaximise="true" version="1"/>

  <optconstraint optconstid="1" prjid="2" name="con1" expression="mean(PIP01.PI12_MIX_MASS_FLOW)" lowerbound="0.1" upperbound="1.0" version="1"/>

  <scenariogenerator scengenid="1" prjid="2" algorithmid="1" extparamvalsetid="1" name="scengentest" version="1"/>
  <scengenobjectivefunction sgobfunctionid="1" scengenid="1" optfunctionid="1" version="1"/>
  <scengenoptconstraint sgoptconstraintid="1" scengenid="1" optconstid="1" version="1"/>

  <decisionvariable decisionvarid="1" scengenid="1" inputid="1" lowerbound="1" upperbound="d/50" typeid="2" version="1"/>

  <modelparameter modelparamid="2" scengenid="1" inputid="2" value="2015-01-01T00:00:00" expression="" version="1"/>
  <modelparameter modelparamid="3" scengenid="1" inputid="3" value="2015-01-01T00:01:00" version="1"/>
  <modelparameter modelparamid="1" scengenid="1" inputid="1" expression="C01.typ" version="1"/>

  <algoparamval aparamsid="1" ascengenid="1" value="1" version="1"/>
  <algoparamval aparamsid="2" ascengenid="1" value="1000" version="1"/>

  <algoparamval aparamsid="3" ascengenid="1" value="1" version="1"/>
  <algoparamval aparamsid="4" ascengenid="1" value="2" version="1"/>
  <algoparamval aparamsid="5" ascengenid="1" value="3" version="1"/>
  <algoparamval aparamsid="6" ascengenid="1" value="2" version="1"/>
  <algoparamval aparamsid="7" ascengenid="1" value="2" version="1"/>
  <algoparamval aparamsid="8" ascengenid="1" value="1" version="1"/>

</dataset>
